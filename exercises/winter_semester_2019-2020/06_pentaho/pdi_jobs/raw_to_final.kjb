    <entry>
      <name>Insert Ratings To Final Layer</name>
      <description />
      <type>SQL</type>
      <sql>INSERT OVERWRITE TABLE imdb_ratings partition(partition_quality='good') 
SELECT r.tconst, r.average_rating, r.num_votes FROM title_ratings r WHERE partition_year = ${year} and partition_month = ${month} and partition_day = ${day} and r.average_rating >= 7;
INSERT OVERWRITE TABLE imdb_ratings partition(partition_quality='worse') 
SELECT r.tconst, r.average_rating, r.num_votes FROM title_ratings r WHERE partition_year = ${year} and partition_month = ${month} and partition_day = ${day} and r.average_rating &lt; 7;</sql>
