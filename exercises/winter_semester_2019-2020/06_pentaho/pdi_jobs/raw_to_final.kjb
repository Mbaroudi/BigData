    <entry>
      <name>Insert Movies To Final Layer</name>
      <description />
      <type>SQL</type>
      <sql>set hive.exec.dynamic.partition.mode=nonstrict;
insert overwrite table imdb_movies partition(partition_year)
select m.tconst, m.title_type, m.primary_title, m.original_title, m.is_adult, m.start_year, m.end_year, m.runtime_minutes, m.genres, m.start_year
from title_basics m WHERE partition_year = ${year} and partition_month = ${month} and partition_day = ${day} 
and m.start_year > 2000;</sql>
